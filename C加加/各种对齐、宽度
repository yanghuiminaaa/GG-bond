#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
	double num[5];
	for (int i = 0; i < 5; i++)
		cin >> num[i];
	for (int i = 0; i < 5; i++)
	{
		cout << left << setw(10)<<fixed << setprecision(2) << num[i] << "|";
		cout << internal << setw(10) <<fixed<< setprecision(2) << num[i] << "|";
		cout << right << setw(10) <<fixed<< setprecision(2) << num[i]  << endl;
	}
	return 0;
}
